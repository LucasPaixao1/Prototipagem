package Controller;

import java.util.List;

import BO.VendasBO;
import VO.VendasVO;

public class VendasController {

	public void cadastrarvendas(VendasVO vendas) throws Exception {
		
		VendasBO vendasBO = new  VendasBO();
		vendasBO.cadastrarVendas(vendas);
		
	}
}

